{
  "name": "endeavour-laptop-rest-api",
  "version": "1.0.0",
  "description": "Endeavour Laptop Rest API",
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && npx tsc && npx tsc-alias -p tsconfig.json",
    "dev": " npx nodemon -x ts-node -e ts -r tsconfig-paths/register src/server.ts",
    "prestart": "npm run build",
    "start": "node dist/src/server.js",
    "preserve": "NODE_ENV=development && npm run build",
    "serve": "concurrently  \"npx tsc -w\" \"npx tsc-alias -w -p tsconfig.json\" \"nodemon dist/src/server.js\""
  },
  "prisma": {
    "seed": "npx ts-node prisma/seeders"
  },
  "keywords": [],
  "author": "Mochammad Fajrin",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.20",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "imagekit": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.10.2",
    "rimraf": "^5.0.5",
    "uuid": "^9.0.1"
  }
}
